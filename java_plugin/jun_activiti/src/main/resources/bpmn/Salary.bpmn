    </sequenceFlow>
    <sequenceFlow id="flow17" name="同意" sourceRef="exclusivegateway4" targetRef="callactivity1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isPass}]]></conditionExpression>
    </sequenceFlow>
